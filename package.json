		"@sveltejs/kit": "^1.27.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"@zerodevx/svelte-img": "^2.1.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"mdsvex": "^0.11.0",
